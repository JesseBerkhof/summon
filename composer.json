{
    "name": "arctic/wraith",
    "description": "Summon a Laravel Package Boilerplate for your next project.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "package",
        "laravel",
        "generator",
        "utility"
    ],
    "authors": [
        {
            "email": "jesseberkhof95@gmail.com",
            "homepage": "https://jesseberkhof.nl",
            "name": "Jesse Berkhof",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/filesystem": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Arctic\\Wraith\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arctic\\Wraith\\WraithServiceProvider"
            ],
            "aliases": {
                "Wraith": "Arctic\\Wraith\\WraithFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "arctic/wraith",
    "description": "Summon a Laravel Package Boilerplate for your next project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "email": "jesseberkhof95@gmail.com",
            "homepage": "https://jesseberkhof.nl",
            "name": "Jesse Berkhof",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/filesystem": "^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Arctic\\Wraith\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arctic\\Wraith\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arctic\\Wraith\\WraithServiceProvider"
            ],
            "aliases": {
                "Wraith": "Arctic\\Wraith\\WraithFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
